{
  "$schema": "https://json.schemastore.org/prettierrc",
  "arrowParens": "always",
  "endOfLine": "auto",
  "printWidth": 100,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "tabWidth": 2,
  "importOrder": [
    "^vue(.*)$",
    "^firebase/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}